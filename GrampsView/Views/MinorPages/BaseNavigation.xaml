<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage
    x:Class="GrampsView.Views.BaseNavigation"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:GrampsView.Views"
    xmlns:fonts="clr-namespace:GrampsView.Resources.Fonts"
    xmlns:views1="clr-namespace:GrampsView.Views.MinorPages"
    FlyoutLayoutBehavior="Popover">

    <FlyoutPage.Flyout>
        <views:FlyoutMenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:HubPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>